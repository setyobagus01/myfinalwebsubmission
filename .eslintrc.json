{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "no-return-assign": 0,
        "no-undef": 0,
        "no-restricted-globals": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "import/extensions": 0,
        "no-unused-vars": 0,
        "class-methods-use-this": 0,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": [
            "error",
            {
            "devDependencies": true,
            "optionalDependencies": false,
            "peerDependencies": false
            }
        ]
    }
}   
